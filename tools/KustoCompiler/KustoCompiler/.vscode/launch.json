{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug runFunctionUpdate",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/../../../bin/debug/KustoCompiler/kc.dll",
            "args": [
                "runFunctionUpdate",
                "-i",
                "${workspaceFolder}/../../../telemetry/KustoQuery"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Debug compile",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/../../../bin/debug/KustoCompiler/kc.dll",
            "args": [
                "compile",
                "-i",
                "${workspaceFolder}/../../../telemetry/KustoSource/",
                "-o",
                "${workspaceFolder}/../../../telemetry/KustoQuery/"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}