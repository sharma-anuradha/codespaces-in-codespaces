{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/rolloutSpecification.json",
  "contentVersion": "1.0.0.0",

  "rolloutMetadata": {
    "serviceModelPath": "{{{component}}}.{{{env}}}-ctl-{{{instance}}}.servicemodel.jsonc",
    "scopeBindingsPath": null,
    "name": "{{{baseInstanceName}}} Rollout 1.0.0.0",
    "rolloutType": "Major",
    "buildSource": {
      "parameters": {
        "versionFile": "buildver.txt"
      }
    }
  },
  "orchestratedSteps": [
    {
      "name": "Deploy {{{baseInstanceName}}}-service",
      "targetType": "ServiceResource",
      "targetName": "{{{baseInstanceName}}}-service",
      "actions": [ "deploy" ]
    }
  ]
}