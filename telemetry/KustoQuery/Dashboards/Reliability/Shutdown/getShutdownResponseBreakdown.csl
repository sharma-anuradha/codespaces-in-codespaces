.create-or-alter function
with (docstring = 'Breakdown of the shutdown request responses, excluding 307.', folder='Dashboards/Reliability/Shutdown')
getShutdownResponseBreakdown
(T:(EventAtShutdownRequestStop_ResponseCode: int))
{
    T
    | where EventAtShutdownRequestStop_ResponseCode != 307
    | summarize count() by EventAtShutdownRequestStop_ResponseCode
}
