{
    "compileOnSave": true,
    "compilerOptions": {
      "baseUrl": ".",
      "composite": true,
      "declaration": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "inlineSources": true,
      "jsx": "react",
      "module": "amd",
      "noEmitHelpers": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "moduleResolution": "node",
      "paths": {
        "*": [
          "../*",
          "*"
        ]
      },
      "outDir": "../../Output/Content/Scripts/ReactViews",
      "rootDir": ".",
      "removeComments": false,
      "sourceMap": true,
      "target": "es5",
      "lib": [
        "dom",
        "es2015.promise",
        "es2015.iterable",
        "es5",
        "scripthost"
      ]
    },
    "include": [
      "**/*"
    ]
  }
