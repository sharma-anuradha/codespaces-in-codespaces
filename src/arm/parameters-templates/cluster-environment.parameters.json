{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "env": {
      "value": "${SERVICE_ENVIRONMENT}"
    },
    "keyVaultAccessPolicies": {
      "value": [
        {
          "objectId": "${SERVICE_KEYVAULT_READER_OBJECTID}",
          "tenantId": "${SERVICE_KEYVAULT_READER_TENANTID}",
          "permissions": {
            "keys": [
              "Get",
              "List"
            ],
            "secrets": [
              "Get",
              "List"
            ],
            "certificates": [
              "Get",
              "List"
            ]
          }
        }
      ]
    },
    "resourceNamePrefix": {
      "value": "${SERVICE_PREFIX}"
    },
    "serviceName": {
      "value": "${SERVICE_NAME}"
    }
  }
}
