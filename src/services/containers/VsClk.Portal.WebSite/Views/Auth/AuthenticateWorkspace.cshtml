@model Microsoft.VsCloudKernel.Services.Portal.WebSite.Models.AuthenticateWorkspaceFormDetails

<p class="vsonline-port-forwarding__status">Connecting to the forwarded port...</p>

<form action="@Model.Action" method="POST" id="tokenForm">
    <input type="hidden" name="cascadeToken" value="@Model.CascadeToken" />
    <input type="hidden" name="featureFlags" value="@Model.FeatureFlags" />
</form>

<script>
    const formElement = document.getElementById("tokenForm");
    formElement.submit();
</script>
