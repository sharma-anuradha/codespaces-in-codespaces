{
  "name": "vso-workbench",
  "version": "0.1.0",
  "main": "dist/src/index.js",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "watch": "webpack -w",
    "compile": "webpack",
    "compile-prod": "webpack --config ./webpack.prod.config.js",
    "setup": "yarn && yarn compile-tsc && yarn update-vscode && yarn compile-prod && yarn copy-build-files",
    "update-vscode": "rimraf node_modules/extension && node scripts/npm/update-vscode.js",
    "copy-build-files": "node scripts/npm/copy-build-files.js",
    "compile-client-core": "cd ../vso-client-core && yarn && cd ../",
    "compile-platform-auth": "cd ../vso-platform-auth && yarn && cd ../",
    "compile-ts-agent": "cd ../vso-ts-agent && yarn && cd ../",
    "preinstall": "yarn compile-client-core && yarn compile-ts-agent && yarn compile-platform-auth",
    "test": "jest --no-cache",
    "test:watch": "jest --no-cache --watch",
    "test:update-snapshots": "jest --no-cache --updateSnapshot",
    "lint": "tslint -c ../tslint.json src/**/*.{ts,tsx} --project tsconfig.json",
    "copy-package-json": "cpx ./package.json ./dist",
    "compile-tsc": "yarn && yarn copy-package-json && tsc -b ./",
    "watch-tsc": "yarn && yarn copy-package-json && tsc -b -watch ./"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test"
    }
  },
  "vscodeCommit": {
    "insider": "1c36d747fbf113144d3bf82b0aa3cbd290714e38",
    "stable": "a9f8623ec050e5f0b44cc8ce8204a1455884749f"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@types/jest": "^25.1.4",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-jest": "^25.2.4",
    "copy-dir": "^1.2.0",
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "lint-staged": "^10.0.7",
    "per-env": "^1.0.2",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sirv-cli": "^1.0.0-next.3",
    "style-loader": "^1.1.3",
    "tar": "^6.0.1",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "typescript": "^3.5.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "vso-client-core": "link:../vso-client-core",
    "vso-ts-agent": "link:../vso-ts-agent",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@vs/vso-splash-screen": "^1.0.1785",
    "debug": "^4.1.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "lint-staged": {
    "*.{css,md,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintIgnore": [
    "build/*"
  ]
}