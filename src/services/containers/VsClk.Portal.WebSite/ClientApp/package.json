  "scripts": {
    "postinstall": "node scripts/npm/postinstall.js",
    "start": "run-p watch:portal watch:service-worker watch:shell",
    "start:portal": "dotnet run --launch-profile Local --project ..",
    "start:webpack-dev-server": "run-p watch:service-worker watch:shell",
    "watch:portal": "dotnet watch --project ../VsClk.Portal.WebSite.csproj run  --launch-profile Local",
    "watch:client": "run-p watch:service-worker watch:shell",
    "watch:service-worker": "cross-env NODE_ENV=development webpack --watch --config webpack.config.worker.js",
    "watch:shell": "react-app-rewired start",
    "build": "run-s build:client",
    "build:client": "run-s build:service-worker build:spa",
    "build:spa": "react-app-rewired build",
    "build:service-worker": "webpack --config webpack.config.worker.js",
    "test": "react-app-rewired test",
    "update-vscode": "node scripts/npm/update-vscode.js",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} --project tsconfig.json",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache"
  },
