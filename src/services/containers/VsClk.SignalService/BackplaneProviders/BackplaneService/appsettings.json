{
  "AppSettings": {
    "Stamp": "usw2",
    "BuildVersion": "unknown",
    "UseTelemetryProvider": true,
    "IsPrivacyEnabled": true,
    "IsAzureDocumentsProviderEnabled": false,
    "WorkerThreads": 200,
    "CompletionPortThreads":  200
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "System.Net.Http.HttpClient": "Error",
      "Microsoft.VsCloudKernel.SignalService": "Debug",
      "Microsoft.VsCloudKernel.BackplaneService": "Debug",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SignalR": "Error",
      "Microsoft.AspNetCore.Http.Connections": "Warning",
      "Microsoft.AspNetCore.SignalR.HubConnectionContext": "Error"
    },
    "Console": {
      "IncludeScopes": true
    }
  }
}
