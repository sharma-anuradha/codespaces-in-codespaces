{
  "AppSettings": {
    "Stamp": "usw2",
    "BuildVersion": "unknown",
    "UseTelemetryProvider": true,
    "IsPrivacyEnabled": true,
    "IsAzureRedisProviderEnabled": true,
    "IsAzureDocumentsProviderEnabled": true,
    "AzureRedisConnectionPool": 6,
    "WorkerThreads": 200,
    "CompletionPortThreads": 200,
    "IsJsonRpcMessagePackEnabled":  true
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error",
      "System": "Error",
      "System.Net.Http.HttpClient": "Error",
      "Microsoft.VsCloudKernel.SignalService": "Information",
      "Microsoft.VsCloudKernel.BackplaneService": "Information",
      "Microsoft": "Error",
      "Microsoft.AspNetCore.SignalR": "Error",
      "Microsoft.AspNetCore.Http.Connections": "Error",
      "Microsoft.AspNetCore.SignalR.HubConnectionContext": "Error"
    },
    "Console": {
      "IncludeScopes": true
    }
  }
}
