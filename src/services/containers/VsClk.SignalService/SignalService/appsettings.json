{
  "AppSettings": {
    "Stamp": "usw2",
    "BuildVersion": "unknown",
    "UseTelemetryProvider": true,
    "IsPrivacyEnabled": true,
    "IsAzureRedisProviderEnabled": true,
    "IsAzureDocumentsProviderEnabled": true,
    "AzureRedisConnectionPool": 3,
    "BackplaneHostName": "bckpln-vsclk-backplaneservice",
    "CorsOrigin": [
      // Dev
      "https://online.dev.core.vsengsaas.visualstudio.com",
      "https://*.dev.core.vsengsaas.visualstudio.com",
      "https://*.app.online.dev.core.vsengsaas.visualstudio.com",

      // PPE
      "https://online-ppe.core.vsengsaas.visualstudio.com",
      "https://*.app.online-ppe.core.vsengsaas.visualstudio.com",

      // PROD
      "https://online.visualstudio.com",
      "https://*.core.vsengsaas.visualstudio.com",
      "https://*.app.core.vsengsaas.visualstudio.com",
      "https://*.app.online.visualstudio.com"
    ]
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error",
      "System": "Error",
      "System.Net.Http.HttpClient": "Error",
      "Microsoft.VsCloudKernel.SignalService": "Warning",
      "Microsoft": "Error",
      "Microsoft.AspNetCore.SignalR": "Error",
      "Microsoft.AspNetCore.Http.Connections": "Error",
      "Microsoft.Azure.SignalR.Common": "Error",
      "Microsoft.Azure.SignalR.ServiceConnection": "None",
      "Microsoft.Azure.SignalR.ServiceRouteHelper": "None",
      "Microsoft.AspNetCore.SignalR.HubConnectionContext": "None"
    },
    "Console": {
      "IncludeScopes": true
    }
  }
}
