{
  "AppSettings": {
    "Stamp": "usw2",
    "BuildVersion": "unknown",
    "UseTelemetryProvider": true,
    "IsPrivacyEnabled": true,
    "IsAzureRedisProviderEnabled": true,
    "IsAzureDocumentsProviderEnabled": true,
    "AzureRedisConnectionPool": 3,
    "BackplaneServiceUri": "http://bckpln-vsclk-backplaneservice/backplanehub",
    "BackplaneJsonRpcServer": "bckpln-vsclk-backplaneservice"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "System.Net.Http.HttpClient": "Error",
      "Microsoft.VsCloudKernel.SignalService": "Debug",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SignalR": "Error",
      "Microsoft.AspNetCore.Http.Connections": "Error",
      "Microsoft.Azure.SignalR.Common": "Error",
      "Microsoft.Azure.SignalR.ServiceConnection": "None",
      "Microsoft.Azure.SignalR.ServiceRouteHelper": "None",
      "Microsoft.AspNetCore.SignalR.HubConnectionContext": "None"
    },
    "Console": {
      "IncludeScopes": true
    }
  }
}
