{
    "extends": "../../config/tsconfig.commonjs.json",
    "compilerOptions": {
        "outDir": "commonjs",
        "baseUrl": "../",
        "skipLibCheck": true,
        "paths": {
            "vso-client-core": [
                "vso-client-core/src/index.ts"
            ],
            "vso-service-worker": [
                "vso-service-worker/src/index.ts"
            ],
            "vso-service-worker-client": [
                "vso-service-worker-client/src/index.ts"
            ],
            "vso-ts-agent": [
                "vso-ts-agent/src/index.ts"
            ]
        }
    },
    "exclude": [
        "commonjs",
        "lib"
    ],
    "include": [
        "package.json",
        "src/**/*"
    ],
    "references": [
        {
            "path": "../vso-client-core/tsconfig.commonjs.json"
        },
        {
            "path": "../vso-ts-agent/tsconfig.commonjs.json"
        },
        {
            "path": "../vso-service-worker/tsconfig.commonjs.json"
        },
        {
            "path": "../vso-service-worker-client/tsconfig.commonjs.json"
        }
    ]
}