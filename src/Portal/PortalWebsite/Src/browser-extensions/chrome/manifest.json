{
  "manifest_version": 2,
  "name": "VSCS GitHub DEV Dashboard",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "declarativeContent"
  ],
  "browser_action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "images/insider.png",
      "32": "images/insider@2x.png",
      "48": "images/insider@3x.png",
      "128": "images/insider@8x.png"
    }
  },
  "icons": {
    "16": "images/insider.png",
    "32": "images/insider@2x.png",
    "48": "images/insider@3x.png",
    "128": "images/insider@8x.png"
  },
  "content_scripts": [{
    "css": [],
    "js": ["content.js"],
    "matches": ["https://github.com/codespaces/*"],
    "run_at": "document_start"
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
